{
  "name": "eslint-plugin-lean-imports",
  "version": "0.3.3",
  "description": "ESLint Plugin to make sure your imports only import the bits you need",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib"
  ],
  "scripts": {
    "test": "npm run lint && npm run build && mocha",
    "build": "rm -rf lib && babel src --out-dir lib",
    "lint": "eslint ./",
    "patch": "release patch",
    "minor": "release minor",
    "major": "release major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eslint-plugins/eslint-plugin-lean-imports.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eslint-plugins/eslint-plugin-lean-imports/issues"
  },
  "homepage": "https://github.com/eslint-plugins/eslint-plugin-lean-imports#readme",
  "peerDependencies": {
    "eslint": ">=2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "~2.2.0",
    "is-my-json-valid": "^2.13.1",
    "mocha": "^2.4.5",
    "mt-changelog": "^0.6.2",
    "release-script": "^1.0.2"
  },
  "keywords": [
    "eslint-plugin",
    "eslintplugin",
    "eslint",
    "import",
    "lodash",
    "react-bootstrap"
  ]
}
